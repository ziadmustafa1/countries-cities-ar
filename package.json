{
  "name": "countries-cities-ar",
  "version": "3.0.1",
  "description": "🌍 Complete library of 250 world countries with 4,642 states/provinces in 3 languages (Arabic, English, French). 100% accurate Arabic translations for all Arab countries. Perfect for React, Next.js, and Vue applications.",
  "type": "module",
  "homepage": "https://countries-cities-ar.vercel.app",
  "bugs": {
    "url": "https://github.com/ziadmustafa1/countries-cities-ar/issues"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "vitest",
    "test:run": "vitest run",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "countries",
    "cities",
    "states",
    "governorates",
    "provinces",
    "arabic",
    "french",
    "localization",
    "world",
    "i18n",
    "multilingual",
    "typescript",
    "react",
    "nextjs",
    "vue",
    "geographic-data",
    "country-codes",
    "iso-countries",
    "world-data",
    "arab-countries",
    "مدن",
    "دول",
    "محافظات"
  ],
  "author": {
    "name": "Ziad Mustafa",
    "url": "https://github.com/ziadmustafa1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ziadmustafa1/countries-cities-ar.git"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ziadmustafa1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^8.0.3",
    "prettier": "^3.2.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
